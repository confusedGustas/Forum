    <changeSet id="insert-realistic-comments-extended" author="gustas">
        <insert tableName="comments">
            <column name="id" value="d0eebc99-9c0b-4ef8-bb6d-6bb9bd380a46"/>
            <column name="text" value="The key is to ensure that AI is developed transparently and with human oversight. We can't afford to create black-box systems that we don't understand."/>
            <column name="created_at" valueComputed="NOW()"/>
            <column name="is_enabled" valueBoolean="true"/>
            <column name="user_id" value="a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a14"/>
            <column name="topic_id" value="c0eebc99-9c0b-4ef8-bb6d-6bb9bd380a31"/>
        </insert>
        <insert tableName="comments">
            <column name="id" value="d0eebc99-9c0b-4ef8-bb6d-6bb9bd380a47"/>
            <column name="text" value="I'm excited about AI's potential in healthcare. It could revolutionize diagnostics and personalized medicine."/>
            <column name="created_at" valueComputed="NOW()"/>
            <column name="is_enabled" valueBoolean="true"/>
            <column name="user_id" value="a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a15"/>
            <column name="topic_id" value="c0eebc99-9c0b-4ef8-bb6d-6bb9bd380a31"/>
        </insert>

        <insert tableName="comments">
            <column name="id" value="d0eebc99-9c0b-4ef8-bb6d-6bb9bd380a48"/>
            <column name="text" value="I think we should strictly limit gene editing to therapeutic purposes, like curing genetic diseases. The idea of 'designer babies' is deeply unsettling."/>
            <column name="created_at" valueComputed="NOW()"/>
            <column name="is_enabled" valueBoolean="true"/>
            <column name="user_id" value="a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a11"/>
            <column name="topic_id" value="c0eebc99-9c0b-4ef8-bb6d-6bb9bd380a36"/>
        </insert>
        <insert tableName="comments">
            <column name="id" value="d0eebc99-9c0b-4ef8-bb6d-6bb9bd380a49"/>
            <column name="text" value="But where do we draw the line between therapy and enhancement? Is correcting a predisposition to a disease an enhancement? It's a slippery slope."/>
            <column name="created_at" valueComputed="NOW()"/>
            <column name="is_enabled" valueBoolean="true"/>
            <column name="user_id" value="a0eebc99-9c0b-4ef8-bb6d-6bb9bd380a12"/>
            <column name="topic_id" value="c0eebc99-9c0b-4ef8-bb6d-6bb9bd380a36"/>
            <column name="parent_comment_id" value="d0eebc99-9c0b-4ef8-bb6d-6bb9bd380a48"/>
        </insert>

